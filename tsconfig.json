{
    "compilerOptions": {
		"strict": true,
        "allowJs": true,
		"checkJs": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
    },
    "include": [
        "./prthgnode/src/**/*.js",
        "./prthgnode/src/**/*.ts"
    ],
    "exclude": [
        "../node_modules/**"
    ]
}